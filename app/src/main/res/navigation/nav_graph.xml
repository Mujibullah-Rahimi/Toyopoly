<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/categoryFragment"
        android:name="no.hiof.toyopoly.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" >
        <argument
            android:name="category"
            app:argType="string"
            android:defaultValue="nocategory" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="no.hiof.toyopoly.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_categoryFragment"
            app:destination="@id/categoryFragment" />
        <action
            android:id="@+id/action_homeFragment_to_CreateAdsFragment"
            app:destination="@id/createAdsFragment" />
    </fragment>

    <fragment
        android:id="@+id/createAdsFragment"
        android:name="no.hiof.toyopoly.CreateAdsFragment"
        android:label="fragment_createAds"
        tools:layout="@layout/fragment_createads" >
        <argument
            android:name="createAds"
            app:argType="string"
            android:defaultValue="nocreateAds" />
    </fragment>
</navigation>